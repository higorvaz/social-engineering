(1). \'cb\'ee\'ec\'e0\'ed\'e8\'e5 \'e2\'ee\'eb\'e8 \'f7\'e5\'eb\'ee\'e2\'e5\'ea\'e0\par
\'cf\'f0\'e8 \'f1\'ee\'ea\'f0\'f3\'f8\'e5\'ed\'e8\'e8 \'e2\'ee\'eb\'e8 \'f7\'e5\'eb\'ee\'e2\'e5\'ea\'e0 \'f5\'ee\'f0\'ee\'f8\'ee \'e4\'e5\'e9\'f1\'f2\'e2\'f3\'fe\'f2 \'ef\'ee\'e1\'ee\'e8, \'e0 \'f2\'e0\'ea\'e6\'e5 \'f3\'ed\'e8\'e7\'e8\'f2\'e5\'eb\'fc\'ed\'fb\'e5 \'ee\'f1\'ea\'ee\'f0\'e1\'eb\'e5\'ed\'e8\'ff \'f1\'eb\'ee\'e2\'ee\'ec \'e8 \'e4\'e5\'eb\'ee\'ec (\'ec\'e0\'ea\'e0\'ed\'e8\'e5 \'e3\'ee\'eb\'ee\'e2\'ee\'e9 \'e2 \'f1\'ee\'f1\'f3\'e4 \'f1 \'e4\'e5\'f0\'fc\'ec\'ee\'ec, \'ef\'f0\'ee\'ec\'e0\'e7\'fb\'e2\'e0\'ed\'e8\'e5 \'ef\'ee\'eb\'ee\'e2\'fb\'ec \'f7\'eb\'e5\'ed\'ee\'ec \'ef\'ee \'e3\'f3\'e1\'e0\'ec, \'f2\'e0\'ea \'ed\'e0\'e7\'fb\'e2\'e0\'e5\'ec\'ee\'e5 \ldblquote\'ee\'ef\'f3\'f1\'e8\'ea\'e0\'ed\'e8\'e5\rdblquote , \'e8\'eb\'e8 \'e8\'e7\'ed\'e0\'f1\'e8\'eb\'ee\'e2\'e0\'ed\'e8\'e5...) \'e2\'ed\'e5\'e7\'e0\'ef\'ed\'ee \'e7\'e0\'f1\'f2\'e0\'e2\'eb\'ff\'fe\'f9\'e8\'e5 \'ee\'e1\'fa\'e5\'ea\'f2 \'ee\'f1\'f2\'f0\'ee \'ef\'f0\'ee\'f7\'f3\'e2\'f1\'f2\'e2\'ee\'e2\'e0\'f2\'fc \'f1\'e2\'ee\'fe \'e1\'e5\'f1\'ef\'ee\'ec\'ee\'f9\'ed\'ee\'f1\'f2\'fc, \'ed\'e8\'f7\'f2\'ee\'e6\'e5\'f1\'f2\'e2\'ee \'e8 \'f3\'ed\'e8\'e6\'e5\'ed\'e8\'e5. \'cf\'f0\'e5\'e4\'e5\'eb\'fc\'ed\'ee \'fd\'f4\'f4\'e5\'ea\'f2\'e8\'e2\'e5\'ed \'e2\'e0\'f0\'e8\'e0\'ed\'f2 \'ef\'f0\'e8\'ed\'f3\'e6\'e4\'e5\'ed\'e8\'ff \'f7\'e5\'eb\'ee\'e2\'e5\'ea\'e0 \'f1\'ee\'e2\'e5\'f0\'f8\'e8\'f2\'fc \'ea\'e0\'ea\'f3\'fe-\'eb\'e8\'e1\'ee \'ef\'ee\'e4\'eb\'ee\'f1\'f2\'fc \'ef\'ee \'ee\'f2\'ed\'ee\'f8\'e5\'ed\'e8\'fe \'ea \'f1\'e2\'ee\'e8\'ec \'e1\'eb\'e8\'e7\'ea\'e8\'ec.\par
\'dd\'f4\'f4\'e5\'ea\'f2\'e8\'e2\'ed\'fb\'ec \'ef\'f0\'e8\'e5\'ec\'ee\'ec \'ef\'ee\'e4\'e0\'e2\'eb\'e5\'ed\'e8\'ff \'e2\'ee\'eb\'e8 \'f1\'f7\'e8\'f2\'e0\'e5\'f2\'f1\'ff \'f2\'e0\'ea\'e6\'e5 \'f1\'ee\'e7\'e4\'e0\'ed\'e8\'e5 \'f2\'e0\'ea\'e8\'f5 \'f1\'e8\'f2\'f3\'e0\'f6\'e8\'e9, \'ef\'f0\'e8 \'ea\'ee\'e8\'f5 \'e6\'e5\'f0\'f2\'e2\'e0 \'ef\'f0\'e8\'f7\'e8\'ed\'ff\'e5\'f2 \'f1\'e5\'e1\'e5 \'e1\'ee\'eb\'fc \'f1\'e0\'ec\'e0. \'dd\'f2\'ee \'e8\'ec\'e5\'e5\'f2 \'ec\'e5\'f1\'f2\'ee, \'e2 \'f7\'e0\'f1\'f2\'ed\'ee\'f1\'f2\'e8, \'ef\'f0\'e8 \'f1\'eb\'e8\'f8\'ea\'ee\'ec \'e4\'ee\'eb\'e3\'ee\'ec \'ef\'f0\'e5\'e1\'fb\'e2\'e0\'ed\'e8\'e8 \'e2 \'ed\'e0\'e2\'ff\'e7\'e0\'ed\'ed\'fb\'f5 \'f1\'f2\'e0\'f2\'e8\'f7\'e5\'f1\'ea\'e8\'f5 \'ef\'ee\'e7\'e0\'f5 (\'f1\'f2\'ee\'ff\'ed\'e8\'e5 \'ed\'e0 \'ed\'ee\'f1\'ea\'e0\'f5, \'e8 \'ed\'e5\'e2\'ee\'e7\'ec\'ee\'e6\'ed\'ee\'f1\'f2\'fc \'ee\'ef\'f3\'f1\'f2\'e8\'f2\'fc\'f1\'ff \'ed\'e0 \'ef\'ee\'e4\'ee\'f8\'e2\'fb \'e8\'e7-\'e7\'e0 \'ee\'f1\'f2\'f0\'fb\'f5 \'f8\'e8\'ef\'ee\'e2 \'ef\'ee\'e4 \'ed\'e8\'ec\'e8...) \'e8\'eb\'e8 \'ea\'ee\'e3\'e4\'e0 \'f1\'e2\'ee\'e8 \'e6\'e5 \'f1\'ee\'e1\'f1\'f2\'e2\'e5\'ed\'ed\'fb\'e5 \'e4\'e2\'e8\'e6\'e5\'ed\'e8\'ff \'e2\'fb\'e7\'fb\'e2\'e0\'fe\'f2 \'f0\'e5\'e7\'ea\'f3\'fe \'ec\'f3\'f7\'e8\'f2\'e5\'eb\'fc\'ed\'f3\'fe \'e1\'ee\'eb\'fc (\'e1\'e5\'e3 \'e3\'ee\'eb\'fb\'ec\'e8 \'f1\'f2\'ee\'ef\'e0\'ec\'e8 \'ef\'ee \'ee\'e1\'eb\'ee\'ec\'e8\'f1\'f2\'fb\'ec \'ea\'e0\'ec\'ed\'ff\'ec \'e8\'eb\'e8 \'ea\'ee\'eb\'fe\'f7\'ea\'e0\'ec...). \'c8\'ed\'ee\'e3\'e4\'e0 \'ed\'e0 \'e3\'ee\'eb\'ee\'e2\'f3 \'ee\'e1\'fa\'e5\'ea\'f2\'e0 \'ed\'e0\'e4\'e5\'e2\'e0\'fe\'f2 \'ec\'e5\'f2\'e0\'eb\'eb\'e8\'f7\'e5\'f1\'ea\'ee\'e5 \'e2\'e5\'e4\'f0\'ee, \'ee\'f2\'f0\'e0\'e6\'e0\'fe\'f9\'e5\'e5 \'e2\'ee\'e7\'ed\'e8\'ea\'e0\'fe\'f9\'e8\'e5 \'e2 \'ed\'e5\'ec \'e7\'e2\'f3\'ea\'e8, \'e8 \'e4\'e5\'e9\'f1\'f2\'e2\'f3\'fe\'f2 \'ed\'e0 \'f7\'e5\'eb\'ee\'e2\'e5\'ea\'e0 \'fd\'eb\'e5\'ea\'f2\'f0\'ee\'f8\'ee\'ea\'ee\'ec, \'ef\'f0\'e8 \'fd\'f2\'ee\'ec \'ea\'f0\'e8\'ea\'e8 \'e6\'e5\'f0\'f2\'e2\'fb \'f1\'f2\'ee\'ea\'f0\'e0\'f2\'ed\'ee \'ee\'f2\'e4\'e0\'fe\'f2\'f1\'ff \'e2 \'e5\'e5 \'ec\'ee\'e7\'e3\'f3 \'e8 \'f2\'e5\'ec \'ee\'f1\'f2\'f0\'e5\'e5, \'f7\'e5\'ec \'f1\'e8\'eb\'fc\'ed\'e5\'e5 \'ee\'ed\'e0 \'ea\'f0\'e8\'f7\'e8\'f2.\par
\'d5\'ee\'f0\'ee\'f8\'ee \'eb\'ee\'ec\'e0\'e5\'f2 \'e2\'ee\'eb\'fe \'ee\'f2\'ea\'eb\'fe\'f7\'e5\'ed\'e8\'e5 \'ee\'f2 \'e2\'f1\'e5\'f5 \'f1\'e5\'ed\'f1\'ee\'f0\'ed\'fb\'f5 \'f0\'e0\'e7\'e4\'f0\'e0\'e6\'e8\'f2\'e5\'eb\'e5\'e9 (\'ef\'ee\'f1\'e0\'e4\'ea\'e0 \'ed\'e0 \'e4\'e5\'ed\'fc-\'e4\'f0\'f3\'e3\'ee\'e9 \'e2 \'f2\'e5\'ec\'ed\'f3\'fe \'e8 \'e0\'e1\'f1\'ee\'eb\'fe\'f2\'ed\'ee \'e7\'e2\'f3\'ea\'ee\'e8\'e7\'ee\'eb\'e8\'f0\'ee\'e2\'e0\'ed\'ed\'f3\'fe \'ea\'ee\'ec\'ed\'e0\'f2\'f3...) \'eb\'e8\'e1\'ee \'e7\'ed\'e0\'f7\'e8\'f2\'e5\'eb\'fc\'ed\'fb\'e9 \'e8\'e7\'e1\'fb\'f2\'ee\'ea \'f1\'e2\'e5\'f2\'e0 (\'e8\'f1\'ef\'ee\'eb\'fc\'e7\'ee\'e2\'e0\'ed\'e8\'e5 \'e1\'ee\'ea\'f1\'e0 \'f1 \'ec\'ee\'eb\'ee\'f7\'ed\'ee-\'e1\'e5\'eb\'fb\'ec\'e8 \'ee\'e4\'ed\'ee\'ee\'e1\'f0\'e0\'e7\'ed\'fb\'ec\'e8 \'f1\'f2\'e5\'ed\'e0\'ec\'e8 \'e8 \'ff\'f0\'ea\'ee \'f1\'e2\'e5\'f2\'ff\'f9\'e8\'ec\'e8 \'ef\'f0\'ee\'e6\'e5\'ea\'f2\'ee\'f0\'e0\'ec\'e8...), \'e0 \'f2\'e0\'ea\'e6\'e5 \'ef\'ee\'eb\'ed\'ee\'e5 \'eb\'e8\'f8\'e5\'ed\'e8\'e5 \'ee\'e1\'fa\'e5\'ea\'f2\'e0 \'f1\'ed\'e0.\par
(2). \'cd\'e0\'ea\'e0\'e7\'e0\'ed\'e8\'e5\par
\'d7\'e0\'f9\'e5 \'e2\'f1\'e5\'e3\'ee \'fd\'f2\'ee \'ee\'f1\'f3\'f9\'e5\'f1\'f2\'e2\'eb\'ff\'e5\'f2\'f1\'ff \'f7\'e5\'f0\'e5\'e7 \'e1\'e0\'ed\'e0\'eb\'fc\'ed\'ee\'e5 \'e8\'e7\'e1\'e8\'e5\'ed\'e8\'e5, \'ea\'ee\'f2\'ee\'f0\'ee\'e5 \'ec\'ed\'ee\'e3\'ee \'f0\'e0\'e7 \'ef\'f0\'ee\'e8\'e7\'e2\'ee\'e4\'e8\'f2\'f1\'ff \'e8\'ec\'ef\'f0\'ee\'e2\'e8\'e7\'e8\'f0\'ee\'e2\'e0\'ed\'ed\'fb\'ec\'e8 \'f1\'f0\'e5\'e4\'f1\'f2\'e2\'e0\'ec\'e8, \'ed\'e5 \'ee\'f1\'f2\'e0\'e2\'eb\'ff\'fe\'f9\'e8\'ec\'e8 \'ff\'e2\'ed\'fb\'f5 \'e2\'ed\'e5\'f8\'ed\'e8\'f5 \'f1\'eb\'e5\'e4\'ee\'e2 (\'ea\'f3\'f1\'ee\'ea \'f0\'e5\'e7\'e8\'ed\'ee\'e2\'ee\'e3\'ee \'f8\'eb\'e0\'ed\'e3\'e0, \'ed\'ee\'f1\'ee\'ea \'e7\'e0\'ef\'ee\'eb\'ed\'e5\'ed\'ed\'fb\'e9 \'ef\'e5\'f1\'ea\'ee\'ec...) \'eb\'e8\'e1\'ee \'f1\'ef\'e5\'f6\'e8\'e0\'eb\'fc\'ed\'fb\'ec\'e8 \'ef\'f0\'e8\'f1\'ef\'ee\'f1\'ee\'e1\'eb\'e5\'ed\'e8\'ff\'ec\'e8, \'f0\'ee\'e6\'e4\'e0\'fe\'f9\'e8\'ec\'e8 \'ef\'ee\'f2\'f0\'ff\'f1\'e0\'fe\'f9\'f3\'fe \'e1\'ee\'eb\'fc (\'f1\'e4\'e5\'eb\'e0\'ed\'ed\'fb\'e5 \'e8\'e7 \'e4\'e5\'f0\'e5\'e2\'e0 \'eb\'ee\'ef\'e0\'f2\'ea\'e8 \'f1\'ee \'ec\'ed\'ee\'e3\'e8\'ec\'e8 \'ee\'f2\'e2\'e5\'f0\'f1\'f2\'e8\'ff\'ec\'e8 \'e2 \'ed\'e8\'f5, \'ef\'eb\'e5\'f2\'e8 \'f1 \ldblquote\'ea\'f0\'ee\'e2\'e0\'e2\'fb\'ec\'e8\rdblquote  \'f3\'e7\'eb\'e0\'ec\'e8....).\par
\'cd\'e0 \'e1\'ee\'eb\'e5\'e2\'fb\'e5 \'ed\'e5\'f0\'e2\'ed\'fb\'e5 \'f0\'e5\'f6\'e5\'ef\'f2\'ee\'f0\'fb \'f1\'f3\'e1\'fa\'e5\'ea\'f2\'e0 \'e2\'ee\'e7\'e4\'e5\'e9\'f1\'f2\'e2\'f3\'fe\'f2 \'ea\'e0\'ea\'e8\'ec\'e8-\'eb\'e8\'e1\'ee \'f0\'e0\'e7\'e4\'f0\'e0\'e6\'e8\'f2\'e5\'eb\'ff\'ec\'e8 \'eb\'e8\'e1\'ee \'e2 \'e5\'f1\'f2\'e5\'f1\'f2\'e2\'e5\'ed\'ed\'ee\'e9 (\'f5\'eb\'b8\'f1\'f2\'fb \'ea\'f0\'e0\'ef\'e8\'e2\'ee\'e9 \'ef\'ee \'ef\'ee\'eb\'ee\'e2\'fb\'ec \'ee\'f0\'e3\'e0\'ed\'e0\'ec...), \'eb\'e8\'e1\'ee \'e2 \'ec\'e5\'e4\'e8\'f6\'e8\'ed\'f1\'ea\'ee\'e9 (\'e2\'ef\'f0\'fb\'f1\'ea\'e8\'e2\'e0\'ed\'e8\'e5 \'ef\'ee\'e4 \'ed\'ee\'e3\'f2\'e8 \'e8\'eb\'e8 \'e2 \'ec\'ee\'f8\'ee\'ed\'ea\'f3 \'fd\'f4\'e8\'f0\'e0...) \'ec\'e0\'ed\'e5\'f0\'e0\'f5. \'dd\'f4\'f4\'e5\'ea\'f2\'e8\'e2\'ed\'ee \'e4\'e5\'e9\'f1\'f2\'e2\'f3\'fe\'f2 \'e8 \'e2\'eb\'e0\'e6\'ed\'fb\'e5 \'ea\'ee\'e6\'e0\'ed\'fb\'e5 \'e1\'f0\'e0\'f1\'eb\'e5\'f2\'fb, \'f1\'e6\'e8\'ec\'e0\'fe\'f9\'e8\'e5 \'ef\'f0\'e8 \'e2\'fb\'f1\'fb\'f5\'e0\'ed\'e8\'e8 \'ef\'e0\'eb\'fc\'f6\'fb.\par
\'ce\'f7\'e5\'ed\'fc \'ec\'f3\'f7\'e8\'f2\'e5\'eb\'fc\'ed\'ee \'e1\'eb\'ee\'ea\'e8\'f0\'ee\'e2\'e0\'ed\'e8\'e5 \'e4\'fb\'f5\'e0\'ed\'e8\'ff, \'e1\'e5\'e7 \'e4\'ee\'e2\'e5\'e4\'e5\'ed\'e8\'ff \'e4\'ee \'ee\'f2\'ea\'eb\'fe\'f7\'e5\'ed\'e8\'ff \'f1\'ee\'e7\'ed\'e0\'ed\'e8\'ff (\'ed\'e5 \'f1\'eb\'e8\'f8\'ea\'ee\'ec \'e4\'eb\'e8\'f2\'e5\'eb\'fc\'ed\'fb\'e5, \'f1\'e5\'ea\'f3\'ed\'e4 \'ef\'ee 30, \'ef\'ee\'e3\'f0\'f3\'e6\'e5\'ed\'e8\'ff \'eb\'e8\'f6\'e0 \'ef\'ee\'e4 \'e2\'ee\'e4\'f3; \'f0\'e5\'e7\'ea\'ee\'e5 \'ee\'f2\'ea\'e8\'e4\'fb\'e2\'e0\'ed\'e8\'e5 \'e3\'ee\'eb\'ee\'e2\'fb \'e8 \'e7\'e0\'eb\'e8\'e2\'e0\'ed\'e8\'e5 \'e2 \'ed\'ee\'e7\'e4\'f0\'e8 \'e2\'ee\'e4\'fb; \'e7\'e0\'f1\'ee\'e2\'fb\'e2\'e0\'ed\'e8\'e5 \'ee\'e1\'fa\'e5\'ea\'f2\'e0 \'e2 \'ef\'eb\'e0\'f1\'f2\'e8\'ea\'ee\'e2\'fb\'e9 \'ec\'e5\'f8\'ee\'ea, \'ed\'e5\'ef\'f0\'ee\'ed\'e8\'f6\'e0\'e5\'ec\'fb\'e9 \'e4\'eb\'ff \'e2\'ee\'e7\'e4\'f3\'f5\'e0...).\par
